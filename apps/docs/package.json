{
  "name": "@ui-lit/doc",
  "version": "1.0.0",
  "private": false,
  "description": "",
  "keywords": [
    "user-interface",
    "lit-element",
    "lit",
    "UI",
    "UI kit",
    "Webcomponent UI"
  ],
  "homepage": "https://github.com/kaifaty/ui-lit/packages/doc",
  "bugs": {
    "url": "https://github.com/kaifaty/ui-lit/issues"
  },
  "license": "MIT",
  "author": "kaifaty",
  "sideEffects": false,
  "type": "module",
  "main": "build/index.js",
  "source": "./src/index.ts",
  "types": "build/index.d.ts",
  "files": [
    "/build",
    "/package.json",
    "/README.md",
    "/src"
  ],
  
  "parcel-namer-struct": [
    {
      "match": "/index.html",
      "dest": "/"
    },
    {
      "type": [
        "png",
        "jpg",
        "svg"
      ],
      "dest": "/images/"
    },
    {
      "type": [
        "woff",
        "woff2"
      ],
      "dest": "/fonts/"
    },
    {
      "type": [
        "js"
      ],
      "dest": "/js/"
    },
    {
      "type": [
        "css"
      ],
      "dest": "/styles/"
    }
  ],
  "scripts": {
    "build": "rimraf ./build/ && rimraf ./dist/ && npm run build:parcel && npm run build:types",
    "build:parcel": "parcel build --target=build",
    "build:types": "tsc",
    "dev": "parcel serve --target=build",
    "test": "tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "browserslist": [
    "last 4 chrome versions"
  ],
  "dependencies": {
    "lit": "^2.6.1",
    "@ui-lit/button": "*",
    "@ui-lit/utils": "*"
  },
  "targets": {
    "build": {
      "context": "browser",
      "distDir": "./build/",
      "source": "./index.html",
      "isLibrary": false,
      "optimize": true,
      "outputFormat": "esmodule"
    }
  }
}
